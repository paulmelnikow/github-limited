{
  "name": "github-limited",
  "description": "Command-line utility for troubleshooting GitHub rate limits",
  "keywords": [
    "github",
    "limit",
    "ratelimit",
    "cli"
  ],
  "version": "1.0.1",
  "license": "MIT",
  "main": "github-limited.js",
  "bin": {
    "github-limited": "cli.js"
  },
  "scripts": {
    "lint": "eslint \"**/*.js\"",
    "prettier-check": "prettier-check \"**/*.@(js|yml|md|json)\"",
    "prettier": "prettier --write \"**/*.@(js|yml|md|json)\"",
    "test": "run-p --continue-on-error lint prettier-check"
  },
  "files": [
    "**/*.js"
  ],
  "repository": "paulmelnikow/github-limited",
  "dependencies": {
    "argparse": "^1.0.9",
    "gh-rate-limit": "^1.0.0",
    "js-yaml": "^3.10.0",
    "lodash.mapvalues": "^4.6.0",
    "moment": "^2.19.3"
  },
  "devDependencies": {
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "prettier-check": "^2.0.0"
  },
  "engines": {
    "node": "8.x | 9.x"
  }
}
